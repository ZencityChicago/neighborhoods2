<script>

  // listen for screen resize events
window.addEventListener('load', function(event){
    // get the width of the screen after the resize event
    var width = document.documentElement.clientWidth;
    
    var tooltip = $('.leaflet-tooltip a');
    // tablets are between 768 and 922 pixels wide
    // phones are less than 768 pixels wide
    if (width < 768) {
        mymap.setZoom(12);
        tooltip.css('font-size', 10);
    }  else {
        mymap.setZoom(13);
        tooltip.css('font-size', 14);
    }
});
      
var mymap = L.map('mapid', {
          center: [41.905461, -87.648077],
          zoom: 13
          });




mymap.doubleClickZoom.disable();
mymap.scrollWheelZoom.disable();

L.tileLayer('https://api.mapbox.com/styles/v1/zencitychicago/cjg2zz8xs0ktb2rqocflctp67/tiles/256/{z}/{x}/{y}@2x?access_token=pk.eyJ1IjoiemVuY2l0eWNoaWNhZ28iLCJhIjoiY2pmbHZxeTM2MGx2ajJ4cW82MTg0eWt1YSJ9.bJfXLNBBWMxyRKDH1FRPGw',
 {
    attribution: 'Map data &copy; <a href="http://openstreetmap.org">OpenStreetMap</a> contributors, <a href="http://creativecommons.org/licenses/by-sa/2.0/">CC-BY-SA</a>, Imagery Â© <a href="http://mapbox.com">Mapbox</a>',
    maxZoom: 16,
    id: 'mapbox.streets',
    accessToken: 'pk.eyJ1IjoiemVuY2l0eWNoaWNhZ28iLCJhIjoiY2pmbHZxeTM2MGx2ajJ4cW82MTg0eWt1YSJ9.bJfXLNBBWMxyRKDH1FRPGw'
}).addTo(mymap);

	// get color depending on population density value
	function getColor(d) {
		return d > 1000 ? '#252525' :
				d > 500  ? '#525252' :
				d > 200  ? '#737373' :
				d > 100  ? '#969696' :
				d > 50   ? '#cdcdcd' :
				d > 20   ? '#d9d9d9' :
				d > 10   ? '#f0f0f0' :
							'#ffffff';
	}

  function style(feature) {
		return {
			weight: 1,
			opacity: 1,
			color: '#999',
			fillOpacity: 0.5,
			fillColor: getColor(feature.properties.density)
		};
	}

	function highlightFeature(e) {
		var layer = e.target;

		layer.setStyle({
			
      weight: 3,
			color: '#777',
			dashArray: '',
			fillOpacity: 0.7
		});

		if (!L.Browser.ie && !L.Browser.opera && !L.Browser.edge) {
			layer.bringToFront();
		}
		info.update(layer.feature.properties);
	}

	var geojson;

	function resetHighlight(e) {
		geojson.resetStyle(e.target);
		info.update();
	}
  
function onclick(e) {
     window.open(e.target.feature.properties.neighborhoodUrl, "_self");
}
function onEachFeature(feature, layer) {
  
   if (feature.properties && feature.properties.popupContent) {   
        layer.bindTooltip(feature.properties.popupContent, {
        permanent: true,
        interactive: true,
        direction: 'center',
        })
        .addTo(mymap);
    }
   if (feature.properties && feature.properties.neighborhoodUrl) { 
    layer.on({
			mouseover: highlightFeature,
		});
    }
		layer.on({
			mouseout: resetHighlight,
      click: onclick,
		});   
	}


